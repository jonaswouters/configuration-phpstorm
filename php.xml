<application>
  <component name="MessDetector">
    <phpmd_settings>
      <MessDetectorConfiguration />
    </phpmd_settings>
  </component>
  <component name="PhpCodeSniffer">
    <phpcs_settings>
      <PhpCSConfiguration max_messages_per_file="100" standards="MySource;PEAR;PHPCS;PSR1;PSR2;Squiz;Zend" tool_path="/usr/local/bin/phpcs" />
    </phpcs_settings>
  </component>
</application>